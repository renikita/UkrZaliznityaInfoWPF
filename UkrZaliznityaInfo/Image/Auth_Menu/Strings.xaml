<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:System="clr-namespace:System;assembly=mscorlib"
                    xmlns:local="clr-namespace:UkrZaliznityaInfo"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="Strings.en.xaml"/>
        <ResourceDictionary Source="Strings.uk.xaml"/>
        <ResourceDictionary Source="Strings.de.xaml"/>
    </ResourceDictionary.MergedDictionaries>
    <!-- English resources -->
    <sys:String x:Key="Registration">Registration</sys:String>
    <!-- Ukrainian resources -->
    <sys:String x:Key="RegistrationUkrainian" xml:lang="uk-UA">Реєстрація</sys:String>
    <!-- German resources -->
    <sys:String x:Key="RegistrationGerman" xml:lang="de-DE">Registrierung</sys:String>
</ResourceDictionary>